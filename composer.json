{
    "name": "8fold/php-shoop",
    "description": "A wrapper to create a more ubiquitous API for interacting with PHP Primitives.",
    "version": "0.1.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Bruce",
            "email": "josh@8fold.pro"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": ">=7.5.6",
        "phpunit/php-code-coverage": "^6.0.7"
    },
    "autoload": {
        "psr-4": {
            "Eightfold\\Shoop\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eightfold\\Shoop\\Tests\\": "./tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true
}
